index=0
def linearsearch(arr,namesearch):
    global index

    for index in range(9):
      if (arr[index] == namesearch):
         return True
    return False




arr=["sarib","ali","ahmed","talha","usman","arhab","waleed","abdullah","basim"]
namesearch = input("enter name to search:")
result = linearsearch(arr,namesearch)
if (result == False):
    print("not found;")
else:
    print("Found at",index+1)
